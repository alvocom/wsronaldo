package variaveis;

public class TiposPrimitivos {
	
	public static void main(String[] args) {
		
		/* Tipos primitivos:
		 * boolean = tipo lógico (tru/false)
		 * char = 1 caracter (apóstofros)  'a' (char) // "a" (String)
		 * byte = -128/+127
		 * short = -32 mil / +32 mil
		 * int = - 9 trilhões / + 9 trilhões
		 * long = quintilhões
		 * float = decimal
		 * double = possui o dobro de precisão em relação ao float
		 */
	}

}
